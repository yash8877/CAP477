import java.util.Scanner;

public class practice {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num;
        int fact = 1;
        num = sc.nextInt();
        if (num == 0) {
            System.out.println("1");
        } else {
            for (int i = 1; i <= num; i++) {
                fact *= i;
            }
            System.out.println(fact);
        }

    }
}
