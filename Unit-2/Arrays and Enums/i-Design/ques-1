import java.util.Scanner;

class Main{
	public static void main(String args[]){  
		Scanner sc =new Scanner(System.in);
		int n;
		n = sc.nextInt();
		int arr[] = new int[n];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = sc.nextInt();
		}
		int sum = 0;
		for (int s : arr) {
			sum += s;
		}
		System.out.println("The sum of the elements in the array is "+sum);
	}  
}
