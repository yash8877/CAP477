import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Enter the string");
        String inputString = scanner.nextLine();
        System.out.println("Enter the end string");
        String endstring = scanner.nextLine();
        if (inputString.endsWith(endstring)) {
            System.out.println("\""+inputString+"\""+" ends with "+"\""+endstring+"\"");
        }
        else{
            System.out.println("\""+inputString+"\""+" does not end with "+"\""+endstring+"\"");
        }
    }
}
